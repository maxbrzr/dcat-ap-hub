@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# =====================================================================
# DATASET: Industrial IoT Fault Detection Dataset
# =====================================================================

<http://example.com/dataset/iiot-fault-detection> a dcat:Dataset ;
    dct:title "Industrial IoT Fault Detection Dataset" ;
    dcat:landingPage <https://www.kaggle.com/datasets/ziya07/industrial-iot-fault-detection-dataset> ;
    dcat:distribution <http://example.com/dist/iiot-fault-detection-zip> ;
    dct:relation 
        <https://github.com/your-org/parser-iiot-fault-detection>,
        <https://github.com/your-org/notebook-iiot-fault-detection> .

<http://example.com/dist/iiot-fault-detection-zip> a dcat:Distribution ;
    dct:title "Dataset Archive (ZIP)" ;
    dcat:downloadURL <https://www.kaggle.com/api/v1/datasets/download/ziya07/industrial-iot-fault-detection-dataset?datasetVersionNumber=1> ;
    dct:format "application/zip" .

<https://github.com/your-org/parser-iiot-fault-detection> a rdfs:Resource ;
    dct:title "Parser: IIoT Fault Detection" ;
    dct:description "Python scripts to parse the IIoT Fault Detection dataset." ;
    dcat:downloadURL <https://raw.githubusercontent.com/your-org/parser-iiot-fault-detection/main/parser.py> ;
    dct:format "text/x-python" .

<https://github.com/your-org/notebook-iiot-fault-detection> a rdfs:Resource ;
    dct:title "Notebook: IIoT Fault Detection" ;
    dct:description "Jupyter Notebook with EDA for the IIoT Fault Detection dataset." ;
    dcat:downloadURL <https://raw.githubusercontent.com/your-org/notebook-iiot-fault-detection/main/notebook.ipynb> ;
    dct:format "application/x-ipynb+json" .