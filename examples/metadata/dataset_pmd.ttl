@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# =====================================================================
# DATASET: IoT Integrated Predictive Maintenance Dataset
# =====================================================================

<http://example.com/dataset/iot-predictive-maintenance> a dcat:Dataset ;
    dct:title "IoT Integrated Predictive Maintenance Dataset" ;
    dcat:landingPage <https://www.kaggle.com/datasets/ziya07/iot-integrated-predictive-maintenance-dataset> ;
    dcat:distribution <http://example.com/dist/iot-predictive-maintenance-zip> ;
    dct:relation 
        <https://github.com/your-org/processor-iot-predictive-maintenance>,
        <https://github.com/your-org/notebook-iot-predictive-maintenance> .

<http://example.com/dist/iot-predictive-maintenance-zip> a dcat:Distribution ;
    dct:title "Dataset Archive (ZIP)" ;
    dcat:downloadURL <https://www.kaggle.com/api/v1/datasets/download/ziya07/iot-integrated-predictive-maintenance-dataset?datasetVersionNumber=1> ;
    dct:format "application/zip" .

<https://github.com/your-org/processor-iot-predictive-maintenance> a rdfs:Resource ;
    dct:title "Processor: IoT Predictive Maintenance" ;
    dct:description "Python scripts to process the IoT Predictive Maintenance dataset." ;
    dcat:downloadURL <https://raw.githubusercontent.com/maxbrzr/dcat-ap-hub/refs/heads/main/examples/processors/processor_pmd.py> ;
    dct:format "text/x-python" .

<https://github.com/your-org/notebook-iot-predictive-maintenance> a rdfs:Resource ;
    dct:title "Notebook: IoT Predictive Maintenance" ;
    dct:description "Jupyter Notebook with EDA for the IoT Predictive Maintenance dataset." ;
    dcat:downloadURL <https://raw.githubusercontent.com/maxbrzr/dcat-ap-hub/refs/heads/main/examples/notebooks/notebook_pmd.ipynb> ;
    dct:format "application/x-ipynb+json" .