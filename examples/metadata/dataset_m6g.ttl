@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# =====================================================================
# DATASET: Intelligent Manufacturing Dataset
# =====================================================================

<http://example.com/dataset/intelligent-manufacturing> a dcat:Dataset ;
    dct:title "Intelligent Manufacturing Dataset" ;
    dct:description "The Intelligent Manufacturing Dataset for Predictive Optimization supports research in smart manufacturing and AI-driven process optimization. It includes real-time sensor data, network metrics, and production records for analyzing and improving manufacturing efficiency." ;
    dcat:landingPage <https://www.kaggle.com/datasets/ziya07/intelligent-manufacturing-dataset> ;
    dcat:distribution <http://example.com/dist/intelligent-manufacturing-zip> ;
    dct:relation 
        <https://github.com/your-org/processor-intelligent-manufacturing>,
        <https://github.com/your-org/notebook-intelligent-manufacturing> .

<http://example.com/dist/intelligent-manufacturing-zip> a dcat:Distribution ;
    dct:title "Dataset Archive (ZIP)" ;
    dcat:downloadURL <https://www.kaggle.com/api/v1/datasets/download/ziya07/intelligent-manufacturing-dataset?datasetVersionNumber=1> ;
    dct:format "application/zip" .

<https://github.com/your-org/processor-intelligent-manufacturing> a rdfs:Resource ;
    dct:title "Processor: Intelligent Manufacturing" ;
    dct:description "Python scripts to process the Intelligent Manufacturing dataset." ;
    dcat:downloadURL <https://raw.githubusercontent.com/maxbrzr/dcat-ap-hub/refs/heads/main/examples/processors/processor_m6g.py> ;
    dct:format "text/x-python" .

<https://github.com/your-org/notebook-intelligent-manufacturing> a rdfs:Resource ;
    dct:title "Notebook: Intelligent Manufacturing" ;
    dct:description "Jupyter Notebook with EDA for the Intelligent Manufacturing dataset." ;
    dcat:downloadURL <https://raw.githubusercontent.com/maxbrzr/dcat-ap-hub/refs/heads/main/examples/notebooks/notebook_m6g.ipynb> ;
    dct:format "application/x-ipynb+json" .